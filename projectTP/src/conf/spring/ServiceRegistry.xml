<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "spring-beans.dtd">

<beans>
	<bean id="FeedBackService" class="com.nimbler.tp.rest.TpFeedbackRestService">
		<property name="loggerName">
			<value>com.nimbler.tp.service.TpFeedbackService</value>
		</property>
	</bean>

	<bean id="TripPlanService" class="com.nimbler.tp.rest.TpPlanRestService">
		<property name="loggerName">
			<value>com.nimbler.tp.service.TpPlanService</value>
