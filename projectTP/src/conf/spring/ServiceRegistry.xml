<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "spring-beans.dtd">

<beans>
	<bean id="FeedBackService" class="com.nimbler.tp.rest.TpFeedbackRestService">
		<property name="loggerName">
			<value>com.nimbler.tp.service.TpFeedbackService</value>
		</property>
	</bean>

	<bean id="TripPlanService" class="com.nimbler.tp.rest.TpPlanRestService">
		<property name="loggerName">
			<value>com.nimbler.tp.service.TpPlanService</value>
		</property>
	</bean>

	<bean id="TpLoggingWS" class="com.nimbler.tp.rest.TpEventLoggingRestService">
		<property name="loggerName">
			<value>com.nimbler.tp.service.TpEventLoggingService</value>
		</property>
	</bean>

	<bean id="TpPredictionService" class="com.nimbler.tp.rest.RealTimePredictionService">
		<property name="loggerName">
			<value>com.nimbler.tp.service.livefeeds</value>
		</property>
	</bean>

	<bean id="userManagementRestService" class="com.nimbler.tp.rest.UserManagementRestService">
		<property name="loggerName">
			<value>com.nimbler.tp.service.twitter</value>
		</property>
	</bean>
	
	<bean id="advisoriesService" class="com.nimbler.tp.rest.AdvisoriesRestService">
		<property name="loggerName">
			<value>com.nimbler.tp.service.twitter</value>
		</property>
	</bean>
	<bean id="gtfsMedaDataResrService" class="com.nimbler.tp.rest.GtfsMetaDataRestService">
		<property name="loggerName">
			<value>com.nimbler.tp.gtfs.GtfsDataMonitor</value>
		</property>
	</bean>
	
	<bean id="TestService" class="com.nimbler.tp.rest.TestService"> </bean>
	
</beans>